[run]
source = src/
omit = 
    */migrations/*
    */venv/*
    */.venv/*
    */tests/*
    */test_*
    manage.py
    */settings/*
    */config/wsgi.py
    */config/asgi.py
    */__pycache__/*
    */node_modules/*
    .tox/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

show_missing = True
skip_covered = False
precision = 2

[html]
directory = htmlcov
title = MultiMarket Hub Coverage Report

[xml]
output = coverage.xml